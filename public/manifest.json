{
  "manifest_version": 3,
  "name": "ES GPT Plugin",
  "version": "0.0.1",
  "short_name": "ES GPT Plugin", 
  "action": {
    "default_popup": "index.html",
    "default_title": "ES GPT Plugin"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png"
  },
  "permissions": [
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://*/"
  ]
}
